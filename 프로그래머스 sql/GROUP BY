고양이와 개는 몇 마리 있을까
SELECT ANIMAL_TYPE, COUNT(*) as count FROM ANIMAL_INS
GROUP BY ANIMAL_TYPE
ORDER BY ANIMAL_TYPE = 'Cat' DESC


동명 동물 수 찾기
SELECT NAME, COUNT(*) as COUNT FROM ANIMAL_INS
WHERE NAME IS NOT NULL
GROUP BY NAME
HAVING COUNT(*) >=2
ORDER BY NAME;


입양 시각 구하기(1)
SELECT HOUR(DATETIME), COUNT(*) FROM ANIMAL_OUTS
WHERE HOUR(DATETIME) >= 9 AND HOUR(DATETIME) < 20
GROUP BY HOUR(DATETIME)
ORDER BY HOUR(DATETIME)


입양 시각 구하기(2)
SET @HOUR = -1; -- HOUR 변수에 -1 저장한 후
SELECT (@HOUR := @HOUR + 1) AS HOUR, (SELECT COUNT(HOUR(DATETIME)) -- HOUR에 1씩 더해서 같은 시간대가 있으면 카운트
                                      FROM ANIMAL_OUTS 
                                      WHERE HOUR(DATETIME)=@HOUR) AS COUNT -- 서브쿼리 이름 COUNT로 지정
FROM ANIMAL_OUTS
WHERE @HOUR <23; -- 22까지 세서 + 1 = 23
